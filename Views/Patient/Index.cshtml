@model IEnumerable<PatientGridItemModel>

<div class="container">
    <h3 class="p-3 text-center">Blazor WebAssembly - Display a list of items</h3>
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>QR Code</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var patient in Model)
            {
                <tr>
                    <td>@patient.Id</td>
                    <td>@patient.Name</td>
                    <td><img height="300" src="data:image/gif;base64,@(patient.QRCode)" /></td>
                </tr>
            }
        </tbody>
    </table>
</div>